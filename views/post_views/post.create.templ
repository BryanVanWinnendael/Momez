package post_views

import "momez/views/components"

templ CreatePost(tags []string) {
	<h1 class="font-bold text-lg leading-tight tracking-tight">Create Post</h1>
	<hr/>
	<div class="w-full h-fit flex flex-col items-center pt-12">
		<div class="w-full h-full flex justify-center items-center">
			@components.CreatePost(tags)
		</div>
	</div>
}
